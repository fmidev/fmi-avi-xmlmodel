<jaxb:bindings version="1.0" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc">

	<jaxb:bindings schemaLocation="https://schemas.wmo.int/iwxxm/3.0.0/iwxxm.xsd" node="//xs:schema">
		<jaxb:schemaBindings>
			<jaxb:package name="icao.iwxxm30"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>

	<jaxb:bindings schemaLocation="https://schemas.wmo.int/iwxxm/3.0.0/spaceWxAdvisory.xsd" node="//xs:schema">
		<jaxb:bindings node="//xs:complexType[@name='SpaceWeatherRegionType']/xs:complexContent/xs:extension/xs:sequence/xs:element[@name='location']">
			<jaxb:property name="geographicLocation"/>
		</jaxb:bindings>
	</jaxb:bindings>

</jaxb:bindings>