<jxb:bindings version="1.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <jxb:bindings schemaLocation="http://www.w3.org/1999/xlink.xsd" node="//xsd:schema">
        <jxb:schemaBindings>
            <jxb:package name="org.w3c.xlink11"/>
        </jxb:schemaBindings>

        <jxb:bindings node=".//xsd:element[@name='title']">
            <jxb:property name="titleElement"/>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>